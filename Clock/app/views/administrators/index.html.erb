<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>

<body>
  <h1>Administrators Index</h1>

  <table>
    <thead>
      <tr>
        <th>idAdmin</th>
        <th>nameAdmin</th>
        <th>emailAdmin</th>
        <th>password</th>
      </tr>
    </thead>
    <tbody>
      <% @administrators.each do |administrator| %>
        <tr>
          <td>
            <%= administrator.id %>
          </td>
          <td>
            <%= administrator.nameAdmin %>
          </td>
          <td>
            <%= administrator.emailAdmin %>
          </td>
          <td>
            <%= administrator.password %>
          </td>
          <td>
            <% end %>
    </tbody>
  </table>
  <button>
    <%= link_to "Manage Employees", administrators_manageEmployees_path %>
  </button> 
  <button>
    <%= link_to "Manage Company Branches", administrators_manageCompany_path %>
  </button>
  <button>
    <%= link_to "Reports", administrators_reports_path %>
  </button>
  <br>
  <button>
    <%= link_to "Home" , root_path%>
  </button>
</body>

</html>