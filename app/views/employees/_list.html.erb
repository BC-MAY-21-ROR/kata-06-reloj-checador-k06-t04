  <table>
    <thead>
      <tr>
        <th>Private Number</th>
        <th>Number</th>
        <th>Name</th>
        <th>Email</th>
        <th>Position</th>
        <th>Attendance Date</th>
        <th>Administrator Id</th>
      </tr>
    </thead>
    <tbody>
      <% @employees.each do |employee| %>
        <tr>
          <td>
            <%= employee.id %>
          </td>
          <td>
            <%= employee.employeeNumber %>
          </td>
          <td>
            <%= employee.nameEmployee %>
          </td>
          <td>
            <%= employee.emailEmployee %>
          </td>
          <td>
            <%= employee.positionEmployee %>
          </td>
          <td>
            <%= employee.attendanceDate %>
          </td>
          <td>
            <%= employee.administrator_id %>
          </td>
          <td>
            <%= link_to "Edit", edit_employee_path(employee), class: "btn btn-warning" %>
          </td>
          <td>
            <%= link_to "Delete", employee_path(employee), method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %>
          </td>

        </tr>
      <% end %>
    </tbody>
  </table>
  